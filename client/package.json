		"@graphql-codegen/typescript-operations": "^4.0.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"pug": "^3.0.2",
		"svelte": "^4.0.0",
		"svelte-check": "^3.4.3",
		"svelte-preprocess": "^5.0.4",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.6"
	},
	"type": "module"
